<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontEnd.HelpWindowPackage.HelpWindowController">
   <children>
      <TreeView fx:id="TW_treeView" onMouseClicked="#selectItem" prefHeight="400.0" prefWidth="200.0" />
      <ScrollPane fx:id="SP_ScrollPane" layoutX="200.0" prefHeight="400.0" prefWidth="400.0">
         <content>
            <TextFlow fx:id="TF_HelpDisplay" prefHeight="400.0" prefWidth="400.0" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
